
#include <stdio.h>

int main () {
float principal,rate,time,compound_interest;

printf("Enter the principal:");
scanf("%f", &principal);
printf("Enter time:");
scanf("%f",&time);
printf("Enter rate:");
scanf("%f", &rate);

compound_interest = principal+((rate+time)/100)*time;
printf("compound_interest = %.2f\n", compound_interest);
    
    return 0;
}
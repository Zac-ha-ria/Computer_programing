
#include <stdio.h>

int main () {
int income,age;

printf("Enter income:");
scanf("%d", &income);

printf("Enter age:");
scanf("%d", &age);

if (age >= 21 && income >= 21000) {
printf("congratulations you quality for a loan");
}
else { 
printf("unfortunately we are unable to offer you a loan at this time");
}
    
    return 0;
}